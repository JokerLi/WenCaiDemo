<?xml version="1.0" encoding="utf-8"?><!-- android:shape指定形状类型，默认为rectangle -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid android:color="#00000000" />
    <padding
        android:bottom="11dp"
        android:left="11dp"
        android:right="11dp"
        android:top="11dp" />
    <corners android:radius="8dp" />
    <stroke
        android:width="1dp"
        android:color="#FF9b9b9d" />
</shape>